CREATE TABLE COMPETENCIA (IDCOMPETENCIA INTEGER AUTO_INCREMENT NOT NULL, ACTIVA TINYINT(1) default 0, NIVEL INTEGER, NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (IDCOMPETENCIA))
CREATE TABLE TIENE (ACTIVO TINYINT(1) default 0, NIVEL INTEGER, COMPETENCIA_IDCOMPETENCIA INTEGER NOT NULL, EMPLEADO_IDEMPLEADO INTEGER NOT NULL, PRIMARY KEY (COMPETENCIA_IDCOMPETENCIA, EMPLEADO_IDEMPLEADO))
CREATE TABLE DEPARTAMENTO (IDDEPARTAMENTO INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, COSTEDEPARTAMENTO FLOAT, NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (IDDEPARTAMENTO))
CREATE TABLE EMPLEADO (IDEMPLEADO INTEGER AUTO_INCREMENT NOT NULL, DTYPE VARCHAR(31), ACTIVO TINYINT(1) default 0, NOMBRE VARCHAR(255), PASSWORD VARCHAR(255), TELEFONO INTEGER, VERSION INTEGER, DEPARTAMENTO_IDDEPARTAMENTO INTEGER, SUELDO INTEGER, HORAS_SEMANA INTEGER, SUELDO_HORA INTEGER, PRIMARY KEY (IDEMPLEADO))
CREATE TABLE COMPETENCIA_EMPLEADO (Competencia_IDCOMPETENCIA INTEGER NOT NULL, empleado_IDEMPLEADO INTEGER NOT NULL, PRIMARY KEY (Competencia_IDCOMPETENCIA, empleado_IDEMPLEADO))
ALTER TABLE TIENE ADD CONSTRAINT FK_TIENE_COMPETENCIA_IDCOMPETENCIA FOREIGN KEY (COMPETENCIA_IDCOMPETENCIA) REFERENCES COMPETENCIA (IDCOMPETENCIA)
ALTER TABLE TIENE ADD CONSTRAINT FK_TIENE_EMPLEADO_IDEMPLEADO FOREIGN KEY (EMPLEADO_IDEMPLEADO) REFERENCES EMPLEADO (IDEMPLEADO)
ALTER TABLE EMPLEADO ADD CONSTRAINT FK_EMPLEADO_DEPARTAMENTO_IDDEPARTAMENTO FOREIGN KEY (DEPARTAMENTO_IDDEPARTAMENTO) REFERENCES DEPARTAMENTO (IDDEPARTAMENTO)
ALTER TABLE COMPETENCIA_EMPLEADO ADD CONSTRAINT FK_COMPETENCIA_EMPLEADO_Competencia_IDCOMPETENCIA FOREIGN KEY (Competencia_IDCOMPETENCIA) REFERENCES COMPETENCIA (IDCOMPETENCIA)
ALTER TABLE COMPETENCIA_EMPLEADO ADD CONSTRAINT FK_COMPETENCIA_EMPLEADO_empleado_IDEMPLEADO FOREIGN KEY (empleado_IDEMPLEADO) REFERENCES EMPLEADO (IDEMPLEADO)
